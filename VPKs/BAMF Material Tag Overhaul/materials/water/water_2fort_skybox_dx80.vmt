"Water"
{
	"%keywords" "tf"
	"%tooltexture" "editor-bamf/waterttx/water/water_2fort_skybox_dx80"

	$forcecheap 1
	
	"%compilewater" 1
	"$abovewater" 1

	$underwateroverlay "effects/water_warp"
	"$envmap" "env_cubemap"

	"$scale" "[1 1]"

	"$bumpmap" "water/dx80_tfwater001_dudv"
	"$normalmap" "water/dx80_tfwater001_normal"

	"$surfaceprop" "water"
	"$bottommaterial" "water/water_2fort_beneath.vmt"
	"$bumpframe" "0"

	"$fogenable" 1
	"$fogcolor" "{51 43 13}"
	"$fogstart" "-100"
	"$fogend" "400"

	"$temp" "[0 0]"
	"$curr" 0.0
	"$curr2" 0.0

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$normalmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}
		"Sine"
		{
			"sineperiod" "24"
			"sinemin" -0.5
			"sinemax" 0.5
			"resultVar" "$curr"
		}
		"Sine"
		{
			"sineperiod" "16"
			"sinemin" 0.5
			"sinemax" -0.5
			"resultVar" "$curr2"
		}
		"Equals"
		{
			"srcVar1" "$curr2"
			"resultVar" "$temp[0]"
		}
		"Equals"
		{
			"srcVar1" "$curr"
			"resultVar" "$temp[1]"
		}
		"TextureTransform"
		{
			"translateVar" "$temp"
			"resultVar" "$bumptransform"
		}
		"WaterLOD"
		{
			"dummy" 0
		}
	}
}
